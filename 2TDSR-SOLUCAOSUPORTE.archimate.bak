  <folder name="Motivation" id="id-8abe87bb1386471594adeddbeb795f37" type="motivation">
    <element xsi:type="archimate:Stakeholder" name="Stakeholder" id="id-d725ac972a46448a82710aa00f68f361"/>
    <element xsi:type="archimate:Stakeholder" name="Presidente da software house(Dilan)" id="id-f20573e3104047f297f186f6cadaa16a"/>
    <element xsi:type="archimate:Driver" name="Acabar com a informalidade nos atendimentos do suporte que estão gerando perda de clientes" id="id-80fa6beb51244cbc8c6c9bbdf0d027ba"/>
    <element xsi:type="archimate:Value" name="Auementar a retenção de clientes atuais, evitando cancelamentos de contratos. Hoje a empresa perde aproximadamente R$200.000 em média por ano, em cancelamentos em função de insatisfação com o suporte técnico da empresa" id="id-851b0a1f97bb4984a1a68a85a6ba343e"/>
    <element xsi:type="archimate:Value" name="Clientes satisfeitos vão ampliar negócios e indicar novos clientes. Hoje, captamos em torno de R$120.00,00 com novos negócios com clientes satisfeitos ou clientes que vieram por indicação" id="id-b8404c75cef24667960fa95ed0935c65">
      <documentation>Clientes satisfeitos vão ampliar negócios e indicar novos clientes. Hoje, captamos em torno de R$120.00,00 com novos negócios com clientes satisfeitos ou clientes que vieram por indicação</documentation>
    </element>
    <element xsi:type="archimate:Assessment" name="Permitir acompanhar todo o processo de movimentação de atendimentos de clientes na área de suporte esteja documentado em um sistema informatizado digital" id="id-f3ef8895c808490294aeb229588c782a"/>
    <element xsi:type="archimate:Assessment" name="Permitir consultar os feedbacks do cliente" id="id-64461b291b5e4f3194686e5f59f232af"/>
    <element xsi:type="archimate:Goal" name="Ter um mecanismo automatizado para registrar pedidos de suporte do cliente" id="id-4f5ce862ac37457cb88804dff02f4ba0"/>
    <element xsi:type="archimate:Driver" id="id-e8f43e99dec245178618743a3e7a1d5d"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-53427638b956449d9a0615810f627c90"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-eaec28c422eb45cdb1992420e99a7c38"/>
    <element xsi:type="archimate:Goal" name="Ter um mecânismo informatizado para registrar as ativadades de solução do chamado" id="id-e3145b8324af461db017d2ad82002403"/>
    <element xsi:type="archimate:Goal" name="Ter um mecanismo de comunicação com o cliente para ter a avaliação do atendimento" id="id-9525aadc4cba4169875072a3c2d2e308"/>
    <element xsi:type="archimate:Outcome" name="100% dos atendimentos registrados em sistema de informação, sendo que essa solução deve estar disponivel a partir de 31/12/2023" id="id-a7ba7070c5b846e6b4aa7520d5346c63"/>
    <element xsi:type="archimate:Outcome" name="As avaliações devem ocorrer após o encerramento do atendimento de cada chamado, sendo que essa função deve estar disponivel 3 meses depois da implantação do sistema de controle de chamado" id="id-527e2490a4394065ae572ef1f13abc15"/>
    <element xsi:type="archimate:Principle" name="Os chamados devem ser resolvidos o mais rapidamente possivel, portanto, devem ser direcionados para grupos de especialistas, conforme o tipo de ocorrencia" id="id-d9c56b6d22054ad283a6384c9d3a3a76"/>
    <element xsi:type="archimate:Principle" name="Um processo de solução deve trabalhar com regras de prazo de atendimento (SLA)" id="id-ea57c51e368d4fc29ee935c17ab36300"/>
    <element xsi:type="archimate:Principle" name="As pesquisas de satisfação devem ser disparadas sempre ao final de um atendimento (após solução do chamado)" id="id-45ae7bdab6c84b3fa5dfed3dc2a19bae"/>
    <element xsi:type="archimate:Principle" name="A resposta pelo cliente é opcional" id="id-9fdd059347e040cead223a712789fb18"/>
